{
  "name": "toolshedr",
  "version": "0.0.0",
  "description": "A tool you need to plan and host efficient workshops",
  "main": "src/app/js/index.js",
  "directories": {
    "doc": "doc",
    "test": "tests/app"
  },
  "scripts": {
    "dev:server": "php -S 127.0.0.1:9001 -t devel/",
    "dev:ui": "grunt",
    "release": "grunt release",
    "start": "npm-run-all --parallel dev:server dev:ui",
    "setup": "echo 'running setup wizard...'",
    "test": "npm run test:local",
    "test:ui": "echo \"Error: no local test specified\" && exit 1",
    "test:server": "phpunit -c tests/server/phpunit.xml",
    "test:server:report": "php -S 127.0.0.1:9002 -t tests/server/results/report",
    "test:codecov": "curl --data-binary @codecov.yml https://codecov.io/validate",
    "watch": "grunt watch"
  },
  "repository": "mkungla/toolshedr",
  "keywords": [
    "workshop"
  ],
  "author": "Marko Kungla",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mkungla/toolshedr/issues"
  },
  "homepage": "https://github.com/mkungla/toolshedr#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-preset-es2015": "^6.16.0",
    "babelify": "^7.3.0",
    "grunt": "^1.0.1",
    "grunt-browserify": "^5.0.0",
    "grunt-contrib-compass": "^1.1.1",
    "grunt-contrib-connect": "^1.0.2",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-contrib-uglify": "^2.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "jshint-stylish": "^2.2.1",
    "npm-run-all": "^3.1.0",
    "whatwg-fetch": "^1.0.0"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": "^3.8.0"
  }
}
